                        <div id="bookbox10" class="display_inline bookbox"><p class="bookbox_text">BookBox 11<span class="delete_bookbox">X</span></p><input type="text"></div>
                    </div>
                </div>
                <div id="desc_legend_container" class="display_inline">
                    <div><div class="desc_legend empty_bookbox display_inline"></div><p class="display_inline">Empty Bookbox</p></div>
                    <div><div class="desc_legend unchanged display_inline"></div><p class="display_inline">Unchanged Content</p></div>
                    <div><div class="desc_legend changed display_inline"></div><p class="display_inline">You Changed Content</p></div>
                    <div><div class="desc_legend input_error display_inline"></div><p class="display_inline">Error with your Input</p></div>
                </div>
